    "# Split the \"Aired Date\" column and store it in \"Start_air_date\" and \"End_air_date\"\n",
    "df[[\"Start_air_date\", \"End_air_date\"]] = df[\"Aired Date\"].str.split(\"-\", expand=True)\n",
    "\n",
    "# Remove leading white spaces from the \"Start_air_date\" column\n",
    "df[\"Start_air_date\"] = df[\"Start_air_date\"].str.strip()\n",
    "df[\"Date\"] = pd.to_datetime(df[\"Start_air_date\"], format=\"%b %d, %Y\")\n",
    "\n",
    "# Extract the month into a new column \"Month\"\n",
    "df[\"Month\"] = df[\"Date\"].dt.strftime(\"%B\")"
   ]

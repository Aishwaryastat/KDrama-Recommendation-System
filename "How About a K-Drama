    "# Group data by year and count the number of dramas for each category\n",
    "individual_counts = individual_data.groupby('Year of release').size()\n",
    "collaboration_counts = collaboration_data.groupby('Year of release').size()\n",
    "\n",
    "# Create a time series plot to visualize changes over time\n",
    "trace_individual = go.Scatter(\n",
    "    x=individual_counts.index,\n",
    "    y=individual_counts.values,\n",
    "    mode='lines+markers',\n",

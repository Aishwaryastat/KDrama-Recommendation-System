   "source": [
    "df1=df.copy()\n",
    "# Split the comma-separated genres into a list and remove whitespace\n",
    "df1['Genre'] = df1['Genre'].str.split(', ')\n",
    "df1['Genre'] = df1['Genre'].apply(lambda genres_list: [genre.strip() for genre in genres_list])\n",
    "\n",
    "# Create a set of unique genres\n",
    "unique_genres = set(genre for genres_list in df1['Genre'] for genre in genres_list)\n",
    "\n",
    "# Create binary columns for each unique genre\n",
    "for genre in unique_genres:\n",
    "    df1[genre] = df1['Genre'].apply(lambda x: 1 if genre in x else 0)\n",
    "\n",
    "\n",
    "genre_data=df1[['Business','Sitcom', 'Adventure', 'Thriller', 'Comedy',\n",
    "                'Action', 'Historical','Melodrama', 'Fantasy', 'Psychological',\n",
    "                'Music', 'Political','Friendship', 'Law', 'Family', 'Food',\n",
    "                'Drama', 'Life', 'Military','Supernatural', 'Sports', 'Crime',\n",
    "                'Medical', 'Horror', 'Mystery','Sci-Fi', 'Romance', 'School', 'Youth']]\n",
    "\n",
    "genre=[]\n",
    "for i in genre_data.columns:\n",
    "  value=(genre_data[i]==1).sum()\n",
    "  genre.append([i,value])\n",
    "count_data=pd.DataFrame(genre,columns=[\"Genre\",\"Count\"])\n",
    "count_data=count_data.sort_values(by=\"Count\")\n",
    "trace=go.Bar(\n",
    "    x=count_data[\"Count\"],\n",
    "    y=count_data[\"Genre\"],\n",
    "    text=count_data[\"Count\"],\n",
    "    textposition=\"outside\",\n",
    "    orientation=\"h\"\n",
    ")\n",
    "\n",
    "layout=go.Layout(\n",
    "    title=\"Genre Counts\",\n",
    "    xaxis=dict(title=\"Genre Count\"),\n",
    "    yaxis=dict(title=\"Genres\")\n",
    ")\n",
    "\n",
    "fig=go.Figure(data=[trace],layout=layout)\n",
    "fig.show()"
   ]

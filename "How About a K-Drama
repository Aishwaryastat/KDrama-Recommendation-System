    "def recommend(kdrama):\n",
    "  index=np.where(df[\"Name\"]==kdrama)[0][0]\n",
    "  similar_drama=sorted(enumerate(cosine_sim[index]),\n",
    "                       key=lambda x:x[1],reverse=True)[1:6]\n",
    "  for i in similar_drama:\n",
    "    print(df[\"Name\"][i[0]])\n"
   ]
  },

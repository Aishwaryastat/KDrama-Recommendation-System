    "# Calculate the average monthly drama releases\n",
    "monthly_averages = df.groupby('Month').size()\n",
    "\n",
    "# Create a bar trace for the monthly averages\n",
    "trace = go.Bar(\n",
    "    x=monthly_averages.index,  # X-axis: months\n",
    "    y=monthly_averages.values,  # Y-axis: average releases for each month\n",
    "    text=monthly_averages.values,  # Display the average value as text on top of the bar\n",
    "    textposition='auto',  # Automatically position the text\n",
    "    marker=dict(color='skyblue'),  # Bar color\n",
    ")\n",
    "\n",
    "# Create a layout\n",
    "layout = go.Layout(\n",
    "    title='Average Monthly Drama Releases',\n",
    "    xaxis=dict(title='Month'),\n",
    "    yaxis=dict(title='Average Releases'),\n",
    ")\n",
    "\n",
    "# Create the figure\n",
    "fig = go.Figure(data=[trace], layout=layout)\n",
    "\n",
    "# Show the plot\n",
    "fig.show()\n"
